import javax.swing.JOptionPane;

public class Menu {
	
	public static void main(String[] args) {
		
		int opcaoEscolhida = Integer.parseInt(JOptionPane.showInputDialog("1-Cadastrar PET\n2-Listar PET\n3-Deletar PET\n4- Listar PETS\n5- Listar PETS por critério\n6- Sair"));
		
		switch(opcaoEscolhida) {
		
		case 1:
			break;
			
		case 2:
			break;
			
			default: System.out.println("Opção Inválida!");
		}
	}
}
	
